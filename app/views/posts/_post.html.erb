<div id="<%= dom_id post %>">
  <p>
    <strong>Titulo:</strong>
    <%= post.title %>
  </p>

  <p>
    <strong>Descrição:</strong>
    <%= post.description %>
  </p>

  <p>
    <strong>Autor:</strong>
    <%= post.author %>
  </p>

  <p>
    <strong>Nota:</strong>
    <%= post.note %>
  </p>
</div>
